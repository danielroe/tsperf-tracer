{
  "publisher": "tsperf",
  "name": "tracer",
  "displayName": "Type Complexity Tracer",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/tsperf/tracer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsperf/tracer"
  },
  "bugs": {
    "url": "https://github.com/tsperf/tracer/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onLanguage:typescript",
    "workspaceContains:tsconfig.json"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "package": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit && pnpm --filter ui typecheck",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@antfu/ni": "^0.21.12",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/d3": "^7.4.3",
    "@types/node": "^18.19.33",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^2.26.1",
    "bumpp": "^9.4.1",
    "d3": "^7.9.0",
    "d3-flame-graph": "^4.1.3",
    "eslint": "^9.2.0",
    "open": "^10.1.0",
    "perfect-debounce": "^1.0.0",
    "rollup": "^4.17.2",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
